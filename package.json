{
  "name": "deepl-translate",
  "version": "0.0.0",
  "type": "module",
  "description": "An unofficial Node package to translate text using [DeepL](https://www.deepl.com).",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=12.20"
  },
  "bin": {
    "deepl": "./lib/cli.js"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "deepl",
    "deepl-api",
    "deepl-free",
    "deepl-translate",
    "deepl-translation",
    "deepl-translator",
    "translate",
    "translate-api",
    "translator"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:r": "r -f cjs",
    "build:ts": "tsc -p tsconfig.lib.json",
    "lint": "run-p lint:*",
    "lint:es": "eslint . --max-warnings 10",
    "lint:tsc": "tsc --noEmit",
    "prepare": "simple-git-hooks || exit 0",
    "test": "vitest run --coverage",
    "typecov": "type-coverage"
  },
  "dependencies": {
    "commander": "^9.3.0",
    "got": "^12.1.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@1stg/lib-config": "^6.1.3",
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "c8": "^7.11.3",
    "type-coverage": "^2.21.1",
    "typescript": "^4.7.2",
    "vitest": "^0.13.0"
  },
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreAsAssertion": true,
    "ignoreNonNullAssertion": true,
    "showRelativePath": true,
    "strict": true,
    "update": true
  }
}
